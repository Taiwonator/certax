---
const { data } = Astro.props;
const { seoTitle, colors, navbar, sections, chatbox } = data.home;
---

<head>
    <title>{seoTitle}</title>
</head>

<style>
    @import url("https://use.typekit.net/kjw7qtq.css");

    * {
        box-sizing: border-box;
        margin: 0;
        padding: 0;
        font-family: questa-sans, sans-serif;
    }

    html {
        scroll-behavior: smooth;
    }

    body {
        animation: fade-in 1s ease-in-out;
        overflow-x: hidden;
    }

    h1,
    h2,
    h3 {
        font-family: questa, serif;
    }

    @keyframes fade-in {
        0% {
            opacity: 0;
        }
        100% {
            opacity: 1;
        }
    }
    body::-webkit-scrollbar {
        width: 10px;
        background-color: #525288;
    }

    body::-webkit-scrollbar-thumb {
        background-color: #e2aa60;
    }

    :root {
        scrollbar-color: #e2aa60 white !important;
        scrollbar-width: thin !important;
    }

    .home-container {
        transition: background-color 0.5s ease-in-out;
    }
    .landing-page-container {
        position: relative;
        height: 100vh;
    }
    .landing-page-container .landing-page-graphics-container {
        position: relative;
        height: 100%;
        z-index: 0;
    }
    .landing-page-container
        .landing-page-graphics-container
        .lower-staircase-container {
        position: relative;
        bottom: 0;
    }

    .header-content-container {
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        display: flex;
        flex-direction: column;
        align-items: center;
        min-width: 80%;
        z-index: 1;
    }
    .company-logo-container svg {
        height: calc(150px + (200 - 150) * ((100vw - 480px) / (1920 - 480)));
    }

    .company-name-container {
        display: flex;
        flex-direction: column;
        align-items: center;
    }
    .company-name-container h1,
    .company-name-container h2 {
        font-weight: 100;
        -webkit-touch-callout: none;
        /* iOS Safari */
        -webkit-user-select: none;
        /* Safari */
        -khtml-user-select: none;
        /* Konqueror HTML */
        -moz-user-select: none;
        /* Old versions of Firefox */
        -ms-user-select: none;
        /* Internet Explorer/Edge */
        user-select: none;
        /* Non-prefixed version, currently
             supported by Chrome, Edge, Opera and Firefox */
    }
    .company-name-container h1 {
        font-size: calc(80px + (115 - 80) * ((100vw - 480px) / (1920 - 480)));
        font-family: questa, serif;
    }
    .company-name-container h2 {
        font-size: calc(28px + (40 - 28) * ((100vw - 480px) / (1920 - 480)));
        line-height: 0.5;
    }
    .company-name-container h2 span {
        display: inline-block;
        position: relative;
        padding: 0px 15px;
        font-family: questa, serif;
    }
    .company-name-container h2 span:before,
    .company-name-container h2 span:after {
        content: "";
        position: absolute;
        height: 5px;
        border-top: 2px solid;
        top: 5px;
        width: 45%;
    }
    .company-name-container h2 span:before {
        right: 100%;
    }
    .company-name-container h2 span:after {
        left: 100%;
    }
    .company-name-container h3 {
        font-size: calc(20px + (23 - 20) * ((100vw - 480px) / (1920 - 480)));
        font-family: questa, serif;
        text-align: center;
        font-weight: 100;
        margin-top: 1.5em;
        max-width: 350px;
    }
    @media (max-width: 1024px) {
        .company-name-container h3 {
            font-weight: 400;
            max-width: 300px;
        }
    }
    .scroll-down-button-container {
        position: relative;
        margin-top: 45px;
        cursor: pointer;
    }
    .scroll-down-button-container svg {
        height: 40px;
        transition: filter 0.2s ease-in-out;
    }
    .scroll-down-button-container svg:hover {
        filter: brightness(0.9);
    }
    .scroll-down-button-container svg:active {
        filter: brightness(0.7);
    }

    .staircase-container {
        position: absolute;
        max-width: 49.5vw;
        width: 550px;
    }
    @media (max-width: 600px) {
        .staircase-container {
            max-width: unset;
            width: 83.5%;
        }
    }
    .staircase-container svg {
        position: absolute;
        width: 100%;
        bottom: 0;
    }
    .staircase-container svg .box-10 {
        opacity: 1;
    }
    .staircase-container svg .box-8 {
        opacity: 0.8;
    }
    .staircase-container svg .box-6 {
        opacity: 0.6;
    }
    .staircase-container svg .box-4 {
        opacity: 0.4;
    }
    .staircase-container svg .box-2 {
        opacity: 0.2;
    }
    @media (max-width: 600px) {
        .staircase-container svg .box-4,
        .staircase-container svg .box-2 {
            display: none;
        }
    }

    .BR {
        right: 0;
        bottom: 0;
    }

    .BL {
        left: 0;
        bottom: 0;
        transform: scaleX(-1);
    }

    .TR {
        top: 0;
        left: 0;
        transform: scaleX(-1) scaleY(-1);
    }

    .TL {
        right: 0;
        top: 0;
        transform: scaleY(-1);
    }

    .background-panels-container {
        height: 100%;
        display: flex;
        flex-direction: row;
        padding: 0 2%;
        justify-content: space-between;
    }
    .background-panels-container .background-panel {
        background-color: white;
        flex: 1;
        margin: 0 5px;
        height: 100px;
        box-shadow:
            0 0px 7px rgba(0, 0, 0, 0.2),
            0 6px 20px 0 rgba(0, 0, 0, 0.19);
        transition: background-color 0.25s linear;
    }
    @media (max-width: 1024px) {
        .background-panels-container .background-panel {
            box-shadow:
                0 0px 7px rgba(0, 0, 0, 0.2),
                0 6px 20px 0 rgba(0, 0, 0, 0.025);
        }
    }
    .background-panels-container .panel-0 {
        height: 95%;
    }
    .background-panels-container .panel-1 {
        height: 85%;
    }
    .background-panels-container .panel-2 {
        height: 75%;
    }
    .background-panels-container .panel-3 {
        height: 65%;
    }
    .background-panels-container .panel-4 {
        height: 45%;
    }
    @media (max-width: 1024px) {
        .background-panels-container .panel-4,
        .background-panels-container .hide {
            display: none;
        }
    }
    @media (max-width: 1024px) {
        .background-panels-container .panel-1 {
            height: 90%;
        }
    }
    @media (max-width: 600px) {
        .background-panels-container .panel-4,
        .background-panels-container .panel-3,
        .background-panels-container .hide {
            display: none;
        }
    }

    .navbar-container {
        position: fixed;
        height: 80px;
        background-color: rgba(255, 255, 255, 0.8);
        width: 100vw;
        box-shadow:
            0 2px 4px -1px rgba(0, 0, 0, 0.06),
            0 4px 5px 0 rgba(0, 0, 0, 0.06),
            0 1px 10px 0 rgba(0, 0, 0, 0.08);
        display: flex;
        align-items: center;
        z-index: 10;
        transition: all 0.5s ease-in-out;
    }
    .navbar-container .navbar-content {
        display: flex;
        flex-direction: row;
        justify-content: space-between;
        align-items: center;
        width: 100%;
        height: 100%;
        padding: 0 0 0 10vw;
    }
    .navbar-container .navbar-items {
        display: none;
    }

    .navbar-active {
        height: 100vh;
        background-color: rgba(255, 255, 255, 0.95);
    }
    .navbar-active .navbar-content {
        display: none;
    }
    .navbar-active .navbar-items {
        display: flex;
        flex-direction: column;
        height: 90%;
        width: 100%;
        justify-content: space-evenly;
        align-items: center;
    }
    @media (max-width: 600px) {
        .navbar-active .navbar-items {
            height: 75%;
        }
    }
    .navbar-active .navbar-items .navbar-items-list {
        width: 100%;
        height: 100%;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
        align-items: center;
    }

    .hiddenNav {
        transform: translateY(-100%);
    }

    @media (max-width: 600px) {
        .company-text-logo-container {
            width: 200px;
        }
    }
    .company-text-logo-container svg {
        height: 35px;
        position: relative;
        top: 5px;
    }
    @media (max-width: 600px) {
        .company-text-logo-container svg .hide {
            display: none;
        }
    }
    @media (max-width: 600px) {
        .company-text-logo-container svg {
            height: 50px;
        }
    }

    .toggle-button-container {
        height: 80px;
        width: 200px;
        position: relative;
        transition: filter 0.2s ease-in-out;
    }
    @media (max-width: 600px) {
        .toggle-button-container {
            width: 100px;
        }
    }
    .toggle-button-container:hover {
        filter: saturate(200%) brightness(0.9);
    }
    .toggle-button-container svg {
        height: 35px;
        position: relative;
        top: 5px;
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
    }

    .bars .cross {
        display: none;
    }

    .cross .bars {
        display: none;
    }

    .inverse svg rect {
        fill: white;
    }

    .navbar-item-container {
        border-width: 0.5px;
        border-style: solid;
        transition: filter 0.2s ease-in-out;
        cursor: pointer;
        -webkit-touch-callout: none;
        /* iOS Safari */
        -webkit-user-select: none;
        /* Safari */
        -khtml-user-select: none;
        /* Konqueror HTML */
        -moz-user-select: none;
        /* Old versions of Firefox */
        -ms-user-select: none;
        /* Internet Explorer/Edge */
        user-select: none;
        /* Non-prefixed version, currently
             supported by Chrome, Edge, Opera and Firefox */
    }
    .navbar-item-container:hover {
        filter: saturate(200%) brightness(0.9);
    }
    .navbar-item-container p {
        font-size: calc(60px + (100 - 60) * ((100vw - 480px) / (1920 - 480)));
        font-family: questa, serif;
        font-weight: 100;
        margin: 0 2vw;
        text-align: center;
    }
    .toggle-switch-container {
        position: relative;
        display: inline-block;
        width: 60px;
        height: 34px;
    }

    .switch input {
        opacity: 0;
        width: 0;
        height: 0;
    }

    .slider {
        position: absolute;
        cursor: pointer;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        -webkit-transition: 0.4s;
        transition: 0.4s;
    }

    .slider:before {
        position: absolute;
        content: "";
        height: 26px;
        width: 26px;
        left: 3px;
        bottom: 3px;
        background-color: white;
        -webkit-transition: 0.4s;
        transition: 0.4s;
    }

    input:checked + .slider {
        background-color: #2196f3;
    }

    input:focus + .slider {
        box-shadow: 0 0 1px #2196f3;
    }

    input:checked + .slider:before {
        -webkit-transform: translateX(26px);
        -ms-transform: translateX(26px);
        transform: translateX(26px);
    }

    /* Rounded sliders */
    .slider.round {
        border-radius: 34px;
    }

    .slider.round:before {
        border-radius: 50%;
    }

    .about-section-container {
        position: relative;
        margin-top: calc(
            200px + (350 - 200) * ((100vw - 480px) / (1920 - 480))
        );
        padding-bottom: 50px;
    }
    .about-section-container .about-section-content-wrapper {
        display: flex;
        flex-direction: column;
        height: 100%;
        width: 100%;
        position: relative;
        align-items: center;
    }
    .about-section-container
        .about-section-content-wrapper
        .about-section-content-container {
        max-width: 1235px;
        width: 60vw;
        height: 688px;
        display: flex;
        margin: 0 auto;
        position: relative;
    }
    @media (max-width: 1024px) {
        .about-section-container
            .about-section-content-wrapper
            .about-section-content-container {
            width: 90vw;
        }
    }
    @media (max-width: 600px) {
        .about-section-container
            .about-section-content-wrapper
            .about-section-content-container {
            width: 90vw;
            height: 800px;
        }
    }

    .content-box-container {
        position: relative;
        width: 90%;
        height: 80%;
        max-width: 1135px;
        background-color: #ffffffbd;
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        display: flex;
        align-items: center;
        justify-content: center;
    }
    @media (max-width: 600px) {
        .content-box-container {
            background-color: unset !important;
        }
    }
    .content-box-container .staircase-container {
        width: 360px;
    }
    @media (max-width: 600px) {
        .content-box-container .staircase-container {
            display: none;
        }
    }
    .content-box-container .content-box-left-half,
    .content-box-container .content-box-right-half {
        position: absolute;
        height: 100%;
        width: 50%;
        border-width: 1px;
        border-style: solid;
        z-index: -1;
    }
    @media (max-width: 600px) {
        .content-box-container .content-box-left-half,
        .content-box-container .content-box-right-half {
            display: none;
        }
    }
    .content-box-container .content-box-left-half {
        left: 0;
    }
    .content-box-container .content-box-right-half {
        right: 0;
    }

    .content-container {
        display: flex;
        max-width: 90%;
        flex-direction: column;
        justify-content: space-around;
        height: 60%;
    }
    @media (max-width: 600px) {
        .content-container {
            align-items: center;
            height: 80%;
        }
    }
    .content-container .header-container {
        position: relative;
    }
    .content-container .header-container h3 {
        font-family: questa, serif;
        font-size: calc(35px + (35 - 35) * ((100vw - 480px) / (1920 - 480)));
        font-weight: 400;
    }
    .content-container p {
        font-size: calc(16px + (18 - 16) * ((100vw - 480px) / (1920 - 480)));
        font-weight: 100;
        line-height: 30px;
    }
    .content-container .buttons-container {
        width: 100%;
        display: flex;
        flex-direction: row;
        justify-content: flex-start;
    }
    @media (max-width: 600px) {
        .content-container .buttons-container {
            flex-direction: column;
        }
    }
    .content-container .buttons-container .button-link .button {
        width: 100%;
    }
    @media (max-width: 600px) {
        .content-container .buttons-container .button {
            margin-top: 5px;
            margin-left: unset !important;
        }
    }

    .button {
        padding: 1em 0;
        font-size: calc(16px + (20 - 16) * ((100vw - 480px) / (1920 - 480)));
        font-weight: 100;
        border-width: 1px;
        border-style: solid;
        text-align: center;
        -webkit-touch-callout: none;
        /* iOS Safari */
        -webkit-user-select: none;
        /* Safari */
        -khtml-user-select: none;
        /* Konqueror HTML */
        -moz-user-select: none;
        /* Old versions of Firefox */
        -ms-user-select: none;
        /* Internet Explorer/Edge */
        user-select: none;
        /* Non-prefixed version, currently
             supported by Chrome, Edge, Opera and Firefox */
        transition:
            filter 0.05s ease-in-out,
            width 0.2s ease-in-out,
            background-color 0.1s ease-in-out;
        min-width: 180px;
        cursor: pointer;
        border: 1px solid white;
    }
    .button:hover {
        filter: saturate(1.5);
    }
    .button:active {
        filter: saturate(2);
    }
    .underline {
        width: 100%;
        height: 100%;
        position: relative;
        display: flex;
    }
    .underline div {
        margin-top: 1em;
        height: 2px;
        width: 30%;
        background-color: black;
    }
    @charset "UTF-8";
    .para,
    .point {
        font-size: calc(16px + (18 - 16) * ((100vw - 480px) / (1920 - 480)));
        font-weight: 100;
        line-height: 36px;
        margin: 0.5em 0;
    }

    ul {
        margin-left: 3em;
    }
    ul .point {
        margin: 0.25em 0 0.25em 2.5em !important;
    }
    ul .point .para:before {
        content: "â€¢";
        font-weight: bold;
        display: inline-block;
        width: 1em;
        margin-left: -1em;
    }

    .bold {
        font-weight: 700 !important;
    }
    canvas {
        position: absolute;
        z-index: -19;
    }

    .info-section-container {
        display: flex;
        flex-direction: column;
        align-items: center;
        padding-top: 50px;
        position: relative;
    }
    .info-section-container .info-section-content-wrapper {
        height: 600px;
        width: 90%;
        display: flex;
        margin: 0 auto;
    }
    .info-section-container
        .info-section-content-wrapper
        .info-section-content {
        flex-basis: 65%;
    }
    @media (max-width: 1024px) {
        .info-section-container
            .info-section-content-wrapper
            .info-section-content {
            flex-basis: 100%;
        }
    }
    @media (max-width: 600px) {
        .info-section-container
            .info-section-content-wrapper
            .info-section-content
            svg {
            display: none;
        }
    }
    .info-section-container
        .info-section-content-wrapper
        .info-section-content
        .content-container {
        height: 80% !important;
    }
    .info-section-container
        .info-section-content-wrapper
        .info-section-content
        .content-container
        p {
        max-width: 80%;
    }
    @media (max-width: 600px) {
        .info-section-container
            .info-section-content-wrapper
            .info-section-content
            .content-container
            p {
            max-width: unset;
        }
    }
    .info-section-container .info-section-content-wrapper .left-side-graphic {
        flex-basis: 25%;
    }
    @media (max-width: 1024px) {
        .info-section-container
            .info-section-content-wrapper
            .left-side-graphic {
            display: none;
        }
    }
    @media (max-width: 600px) {
        .info-section-container
            .info-section-content-wrapper
            .left-side-graphic {
            display: none;
        }
    }
    .info-section-container .middle-graphic-container {
        width: 150px;
        position: relative;
    }
    .SVGCalculator-container {
        display: flex;
    }
    .SVGCalculator-container svg {
        height: calc(200px + (350 - 200) * ((100vw - 480px) / (1920 - 480)));
    }

    .info-section-content {
        display: flex;
        flex-direction: row;
        position: relative;
        justify-content: space-evenly;
        align-items: center;
    }
    .testimonial-section-container {
        position: relative;
        text-align: center;
    }

    .subheader-container {
        margin: 3em auto;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        padding: 0 1em;
        position: relative;
        max-width: 720px;
    }
    .subheader-container h3 {
        font-weight: 100;
        font-size: calc(35px + (70 - 35) * ((100vw - 480px) / (1920 - 480)));
    }
    .subheader-container h4 {
        font-weight: 100;
        font-size: 20px;
        font-family: questa, serif;
        margin: 20px 0 0 10px;
    }
    .subheader-container .stars {
        width: 40%;
        margin-top: 2em;
    }

    .hide * {
        display: none;
        display: none;
    }

    .big-testimony-container {
        position: relative;
        display: flex;
        width: 80%;
        min-height: 400px;
        margin: 0 auto;
    }
    @media (max-width: 1024px) {
        .big-testimony-container {
            flex-direction: column;
            margin-bottom: 40px;
        }
    }
    .big-testimony-container .big-testimony-content-container {
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 2em;
        flex-basis: 60%;
    }
    @media (max-width: 1024px) {
        .big-testimony-container .big-testimony-content-container {
            order: 0 !important;
        }
    }
    .big-testimony-container .big-testimony-image {
        flex-basis: 40%;
        min-height: 400px;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
        filter: brightness(0.8) blur(0.2px);
    }
    .quote-container {
        display: flex;
        flex-direction: column;
    }
    .quote-container p {
        font-size: 17px;
        color: white;
        font-weight: 100;
        line-height: 45px;
    }
    .quote-container p:before,
    .quote-container p:after {
        content: '"';
    }
    .quote-container h4 {
        font-size: 18px;
        text-transform: uppercase;
        max-width: 85%;
    }
    .quote-container h4 span {
        display: inline-block;
        position: relative;
        padding: 0px 15px;
    }
    .quote-container h4 span:before,
    .quote-container h4 span:after {
        content: "";
        position: absolute;
        height: 5px;
        border-top: 4px solid;
        top: 50%;
        width: 15%;
    }
    .quote-container h4 span:before {
        right: 100%;
    }
    .quote-container h4 span:after {
        left: 100%;
    }
    .quote-container h5 {
        font-size: 16px;
        font-weight: 400;
        padding: 0 15px;
    }

    .hide-left span:before {
        display: none;
    }

    .hide-right span:after {
        display: none;
    }

    .quote-carousel-container {
        height: 200px;
        position: relative;
        display: flex;
        flex-direction: row;
        margin-top: 10px;
    }
    .quote-carousel-container .quote-carousel-button {
        position: relative;
        width: 10vw;
        height: 100%;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .quote-carousel-container .quote-carousel-button:hover {
        filter: brightness(0.8);
    }
    .quote-carousel-container #back {
        cursor: pointer;
    }
    .quote-carousel-container #back p {
        font-size: calc(16px + (18 - 16) * ((100vw - 480px) / (1920 - 480)));
        transform: rotate(270deg);
        -webkit-touch-callout: none;
        /* iOS Safari */
        -webkit-user-select: none;
